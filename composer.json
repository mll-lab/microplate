{
  "name": "mll-lab/microplate",
  "type": "library",
  "description": "PHP package to easily work with microplate data",
  "homepage": "https://github.com/mll-lab/microplate",
  "license": "MIT",
  "authors": [
    {
      "name": "Simon Bigelmayr",
      "email": "s.bigelmayr@gmx.net"
    }
  ],
  "require": {
    "php": "^7.4 || ^8",
    "illuminate/support": "^7 || ^8",
    "myclabs/php-enum": "^1.7",
    "thecodingmachine/safe": "^1"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2",
    "infection/infection": "~0.25",
    "jangregor/phpstan-prophecy": "^1",
    "laravel/framework": "^7 || ^8",
    "mll-lab/php-cs-fixer-config": "^4.3",
    "nunomaduro/larastan": "^1.0",
    "orchestra/testbench": "^5 || ^6",
    "phpstan/extension-installer": "^1",
    "phpstan/phpstan": "^1",
    "phpstan/phpstan-deprecation-rules": "^1",
    "phpstan/phpstan-phpunit": "^1",
    "phpstan/phpstan-strict-rules": "^1",
    "phpunit/phpunit": "^9",
    "rector/rector": "^0.12.2",
    "symfony/var-dumper": "^5",
    "thecodingmachine/phpstan-safe-rule": "dev-master#a630c7bf9f3060b6e3dc74a4d9ef5edc0feec052"
  },
  "config": {
    "platform": {
      "php": "7.4.24"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Mll\\Microplate\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mll\\Microplate\\Tests\\": "tests/"
    },
    "files": [
      "vendor/symfony/var-dumper/Resources/functions/dump.php"
    ]
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "thecodingmachine/phpstan-safe-rule",
        "type": "library",
        "version": "dev-master",
        "dist": {
          "url": "https://codeload.github.com/thecodingmachine/phpstan-safe-rule/legacy.zip/a630c7bf9f3060b6e3dc74a4d9ef5edc0feec052",
          "type": "zip"
        },
        "autoload": {
          "psr-4": {
            "TheCodingMachine\\Safe\\PHPStan\\": "src/"
          }
        }
      }
    }
  ],
  "support": {
    "issues": "https://github.com/mll-lab/microplate/issues",
    "source": "https://github.com/mll-lab/microplate"
  }
}
